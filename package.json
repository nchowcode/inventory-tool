{
  "name": "inventory-manager",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "deploy": "npm run build && gcloud functions deploy processEmail --runtime nodejs18 --trigger-http --allow-unauthenticated --source dist",
    "dev": "npx functions-framework --target=processEmail --source=dist/index.js",
    "watch": "tsc --watch",
    "start": "npm run build && npm run dev"
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.1.0",
    "@google-cloud/functions-framework": "^3.3.0",
    "@google-cloud/pubsub": "^4.0.7",
    "firebase-admin": "^12.0.0",
    "googleapis": "^129.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
